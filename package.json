{
  "name": "labuona-api",
  "version": "0.0.1",
  "description": "La Buonapasta API",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap run",
    "start": "pnpm run build:ts && fastify start -l info dist/main/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/main/app.js",
    "prettier:check": "prettier . --check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/sensible": "^5.5.0",
    "fastify": "^4.24.3",
    "fastify-cli": "^5.8.0",
    "fastify-plugin": "^4.5.1",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.26.3",
    "pg": "^8.11.3",
    "tsx": "^3.14.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@tapjs/test": "^1.3.13",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^18.18.6",
    "@types/pg": "^8.10.7",
    "@types/tap": "^15.0.10",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "9.0.0",
    "fastify-tsconfig": "^1.0.1",
    "prettier": "3.0.3",
    "tap": "^18.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "tap": {
    "before": "./test/before.ts",
    "node-arg": [
      "--env-file=./.env.test"
    ]
  },
  "volta": {
    "node": "20.7.0",
    "npm": "10.1.0"
  }
}
