{
  "name": "labuona-api",
  "version": "0.0.1",
  "description": "La Buonapasta API",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "pnpm run build:ts && tsc -p test/tsconfig.json && tap -w --ts \"{src,test}/**/*.test.ts\"",
    "start": "pnpm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "prettier:check": "prettier . --check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "9.0.0",
    "fastify-tsconfig": "^1.0.1",
    "prettier": "3.0.3",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "volta": {
    "node": "20.7.0",
    "npm": "10.1.0"
  }
}
